{
  "name": "javascript",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.1"
  },
  "scripts": {
    "kotlin": "npm run kotlin:build && npm run kotlin:copy",
    "kotlin:build": "../kotlin/gradlew build -p ../kotlin",
    "kotlin:copy": "cp -r ../kotlin/web/*.js ./public/kotlin",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
